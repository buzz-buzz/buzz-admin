    "koa-basic-auth": "^2.0.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "0.0.16",
    "koa-react-view": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static-cache": "^5.1.2",
